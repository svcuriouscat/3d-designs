detail 512
rotate 0  0  -0.5  /// CAD mode

define DIAMETER_OUTER  11       /// mm
define DIAMETER_INNER   6.35    /// mm
define THICKNESS        0.2     /// mm

define WASHER {
    difference {
        cylinder {
            size DIAMETER_OUTER  THICKNESS
        }

        cylinder {
            size (DIAMETER_INNER + 0.4)  THICKNESS
        }
    }
}

WASHER {
}
